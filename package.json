{
  "name": "steno-hybrid",
  "version": "2.1.0",
  "description": "Specialized fast async file writer",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib/esm/index.js",
    "lib/cjs/index.js",
    "lib/types/index.d.ts"
  ],
  "types": "./lib/types/index.d.ts",
  "keywords": [
    "fs",
    "file",
    "write",
    "writer",
    "asynchronous",
    "fast",
    "race",
    "condition",
    "atomic",
    "writing",
    "safe"
  ],
  "homepage": "https://github.com/crashmax-dev/steno-hybrid",
  "bugs": {
    "url": "https://github.com/crashmax-dev/steno-hybrid/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crashmax-dev/steno-hybrid.git"
  },
  "funding": "https://github.com/sponsors/typicode",
  "license": "MIT",
  "author": "Typicode <typicode@gmail.com>",
  "scripts": {
    "test": "npm run build && xv lib",
    "build": "del-cli lib && concurrently npm:build:* && bash ./scripts/hybrid.sh",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "lint": "eslint src --ext .ts --ignore-path .gitignore",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags && npm publish",
    "benchmark": "npm run build && node lib/benchmark.js",
    "commit": "commit"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt-cli": "^13.1.0",
    "@sindresorhus/tsconfig": "^1.0.2",
    "@types/node": "^16.6.1",
    "@typicode/eslint-config": "^0.1.2",
    "concurrently": "^6.3.0",
    "del-cli": "^4.0.1",
    "husky": "^7.0.1",
    "typescript": "^4.3.5",
    "xv": "^0.2.3"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  }
}
